{
  "env": {
    "browser": false,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "object-curly-newline": "off",
    "consistent-return": "off",
    "quotes": "off",
    "no-underscore-dangle": "off",
    "spaced-comment": "off",
    "import/newline-after-import": "off",
    "no-console": "off",
    "eol-last": "off"
  }
}
